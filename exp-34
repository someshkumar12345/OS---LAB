#include <stdio.h>

int main() {
    int f[50], i, st, len, j, c;
    for (i = 0; i < 50; i++) f[i] = 0;

    printf("Enter starting block: ");
    scanf("%d", &st);
    printf("Enter length of file: ");
    scanf("%d", &len);

    for (j = st; j < (st + len); j++) {
        if (f[j] == 0) f[j] = 1;
        else {
            printf("Block %d already allocated\n", j);
            return 0;
        }
    }

    printf("File allocated at blocks: ");
    for (j = st; j < (st + len); j++) printf("%d ", j);
    printf("\n");
    return 0;
}
